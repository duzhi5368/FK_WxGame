/***********************************************
    *子表定义  
***********************************************/
.mc-grid-group { /* grid has group*/
	background-color:#F9F9F9; background-clip:padding-box;border-radius: 0.3125rem; border:solid 1px #DDD;
}
.mc-grid-group .mc-group .mc-row { /* group row margin-right */
	margin-right: 0.5rem; 
}
.mc-grid-group .mc-group:last-child {
	padding-bottom: 0.5rem;
}
.mc-grid-group .mc-group:after { /* group's split line */
	content: '';display:block;
	margin:0.5rem 0 0 0; height:0.5rem;background-color:#EEE;
}
.mc-grid-group .mc-group:last-child:after { /* delete last group's split line*/
	margin:0;height:0;
}